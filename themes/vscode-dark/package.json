{
  "name": "slidev-theme-vscode-dark",
  "version": "0.0.0",
  "type": "module",
  "engines": {
    "node": ">=24"
  },
  "scripts": {
    "build": "slidev build example.md",
    "dev": "slidev example.md --open",
    "export": "slidev export example.md",
    "screenshot": "slidev export example.md --format png"
  },
  "dependencies": {
    "@slidev/types": "^52.11.5"
  },
  "devDependencies": {
    "@slidev/cli": "52.12.0"
  },
  "slidev": {
    "colorSchema": "dark",
    "highlighter": "shiki",
    "defaults": {
      "fonts": {
        "sans": "Segoe UI, sans-serif",
        "mono": "Fira Code"
      }
    }
  }
}
