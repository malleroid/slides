{
  "name": "slidev-theme-paiza",
  "version": "0.0.0",
  "type": "module",
  "engines": {
    "node": ">=24"
  },
  "scripts": {
    "build": "slidev build example.md",
    "dev": "slidev example.md --open",
    "export": "slidev export example.md",
    "screenshot": "slidev export example.md --format png"
  },
  "dependencies": {
    "@iconify-json/twemoji": "^1.2.0",
    "@slidev/types": "^52.11.5"
  },
  "devDependencies": {
    "@slidev/cli": "52.12.0"
  },
  "//": "Learn more: https://sli.dev/guide/write-theme.html",
  "slidev": {
    "colorSchema": "both",
    "highlighter": "shiki",
    "defaults": {
      "fonts": {
        "sans": "M PLUS 2",
        "mono": "Fira Code"
      }
    }
  }
}